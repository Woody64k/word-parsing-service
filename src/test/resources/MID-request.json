{
	"values": [
		{
			"searchTerm": "Bedarfsanforderungsnummer *",
			"resultName": "anforderungsNummer",
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Bedarfstr√§ger",
			"values": [
				{
					"searchTerm": "Ansprechpartner",
					"resultName": "kontakt",
					"defaultValue": "N/A"
				},
				{
					"searchTerm": "Vertreter",
					"resultName": "vertretung",
					"defaultValue": "N/A"
				}
			]
		},
		{
			"searchTerm": "Leistungsumfang / Aufgabenbeschreibung:",
			"resultName": "leistungsumfang",
			"transform": {
				"subString": {
					"startWords": [
						"Leistungsumfang / Aufgabenbeschreibung"
					],
					"endWords": [
						"Umzusetzende RfCs / Arbeitspakete und Beauftragung des externen Dienstleisters"
					]
				}
			},
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Produkt- / Projektbezeichnung des BV *",
			"resultName": "projekt",
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Ende Angebotsabgabe",
			"resultName": "angebotsfrist",
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Vertragsnummer",
			"resultName": "vertragsNummer",
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Vertragspartner",
			"resultName": "vertragsPartner",
			"defaultValue": "N/A"
		},
		{
			"searchTerm": "Kontraktnummer",
			"resultName": "kontraktNummer",
			"transform": {
				"subString": {
					"startWords": [
						"*"
					]
				},
				"split": {
					"by": "/",
					"as": "list"
				}
			}
		}
	],
	"lists": [
		{
			"values": [
				{
					"searchTerm": "Rollen bzw. konkrete Skillprofile",
					"transform": {
						"split": {
							"by": ",",
							"as": "values",
							"values": [
								{
									"position": 1,
									"resultName": "los"
								},
								{
									"position": 2,
									"resultName": "preisgruppe",
									"transform": {
										"subString": {
											"startWords": [
												"PG"
											]
										}
									}
								},
								{
									"position": 3,
									"resultName": "stelle",
									"transform": {
										"subString": {
											"endWords": [
												"Remotezeit",
												"Servicezeit"
											]
										}
									}
								},
								{
									"fromPosition": 3,
									"resultName": "detail"
								}
							]
						}
					}
				},
				{
					"searchTerm": "Anzahl",
					"resultName": "anzahl"
				},
				{
					"searchTerm": "Aufwand\n in Std.",
					"resultName": "stunden"
				},
				{
					"searchTerm": "geplanter Starttermin",
					"resultName": "startTermin"
				},
				{
					"searchTerm": "geplanter Endetermin",
					"resultName": "endeTermin"
				}
			],
			"resultName": "stellen",
			"transform": {
				"filter": {
					"and": [
						{
							"value": "startTermin",
							"isEmpty": false
						},
						{
							"value": "endeTermin",
							"isEmpty": false
						}
					]
				},
				"merge": {
					"by": [
						"los",
						"preisgruppe",
						"stelle",
						"startTermin",
						"endeTermin"
					],
					"objects": [
						{
							"resultName": "budgets",
							"values": [
								"stunden",
								"anzahl",
								"detail"
							]
						}
					]
				}
			}
		}
	]
}